package com.wkcto.crm.utils;

import java.util.UUID;

/**
 * UUID生成器
 * @author Administrator
 *
 */
public class UUIDGenerator { // 生成器（名词）
	
	private UUIDGenerator(){}
	
	/**
	 * 生成UUID
	 * @return
	 */
	public static String generate(){ // 生成（动词）
		return UUID.randomUUID().toString().replaceAll("-", "");
	}
	
}
